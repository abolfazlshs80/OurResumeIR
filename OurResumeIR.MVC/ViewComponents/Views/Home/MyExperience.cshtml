@model IEnumerable<OurResumeIR.Domain.Models.History>
<h1 class="section-title container">تجربه های من</h1>
<div id="experience" class="experience container col-11 col-md-12">

	<div class="experience-title col-12 col-sm-12 col-md-6">
        
        @foreach (var item in Model.Take(Model.Count()/2))
        {
	        <h1>
		        <i class="bi bi-lightning-charge-fill"></i>
		      @item.Name
	        </h1>
        }
	

	</div>

	<div class="experience-title col-12 col-sm-12 col-md-6">
		@foreach (var item in Model.Skip(Model.Count() / 2))
		{
			<h1>
				<i class="bi bi-lightning-charge-fill"></i>
				@item.Name
			</h1>
		}
	

	</div>

</div>@*
