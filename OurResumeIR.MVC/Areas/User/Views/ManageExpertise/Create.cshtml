@model OurResumeIR.Application.ViewModels.Experience.CreateExpertiseLayerVM 

@using Microsoft.AspNetCore.Mvc.TagHelpers
@{
	Layout = "_ProfileLayout";
	ViewBag.titlepage = "سطح تخصص";
}


<div class="col-xl-9">
	<div class="row">
		<div class="col-md-4">

     
			<form method="post" asp-action="Create" asp-controller="ManageExpertise">

				@Html.ValidationSummary()
				<div class="form-group">
					@Html.LabelFor(a=>a.Name)
					@Html.EditorFor(a => a.Name, new { @class = "form-control " })
	
				</div>
				<div class="form-group">
					<input type="submit" value="ثبت" class="btn btn-success" />
				</div>
			</form>
		</div>
	</div>
</div>

@section Scripts {
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
